<body>
    <h4>Today's Leads</h4>
     <table id="table1" class="table table-striped table-bordered nowrap" style="width:100%;text-align:center">
                            <thead>
                                <tr class="danger">
                                    <th>Lead Name</th>
                                    <th>Mobile</th>
                                    <th>Email</th>
                                    <th>Address</th>
                                    <th>Product</th>
                                    <th>Reference</th>
