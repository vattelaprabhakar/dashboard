                                    <th>Assigned By</th>
                                    <th>Status</th>
                                    <th>Lead Owner</th>
                                    <th>Website</th>
                                    <th>Industry</th>
                                    <th>Rating</th>
                                    <th>Lead Assigned To</th>
                                    <th>Lead Created Date</th>
                                    <th>Lead Modified Date</th>

                               </tr>
                            </thead>
                            <tbody></tbody>
                            </table>

    <script type="text/javascript">
        $(document).ready(function () {

            $("#FromDate").datepicker().datepicker("setDate", new Date());
            $("#ToDate").datepicker().datepicker("setDate", new Date());

          
             
                $('#table1').DataTable({
                    ajax: {
                        url: "/Reports/GetCurrentDateRecords",
                        type: "get",
                        datatype: "json",
                        dataSrc: "",
                        data: { fromdate: fdate, todate: tdate },
                        scrollX: true,
                        scrollY: true,
                        responsive: true,
                        scrollCollapse: true,
                        paging: true
                    },
                    columns: [
                                { 'data': 'LeadName' },
                                 { 'data': 'Mobile' },
                                  { 'data': 'Email' },
                                   { 'data': 'Address' },
                                    { 'data': 'Product' },
                                      { 'data': 'Reference' },
                                        { 'data': 'Wantalerts' },
                                          { 'data': 'LeadAssignedby' },
                                            { 'data': 'Status' },
                                              { 'data': 'LeadOwner' },
                                                { 'data': 'Website' },
                                                  { 'data': 'Industry' },
                                              { 'data': 'Rating' },
                                                { 'data': 'LeadAssignedTo' },
                                                  { 'data': 'CreatedDate' },
                                                   { 'data': 'ModifiedDate' },
                    ]
                });

            });

  

