        <div class="table-responsive" style="margin-top: 2px;">
            <table id="myTable" class="table table-striped table-bordered nowrap">
                <thead>
                    <tr class="danger">
                        <th>Buttons</th>
                        <th>Lead Date</th>
                        <th>Lead Name</th>
                        <th>Mobile</th>
                        <th>Product</th>
                        <th>Source</th>
                        <th>Comment1</th>
                        <th>Status</th>
                        <th>Lead Owner</th>
                        <th>Followupdate</th>
                        <th>Assigned By</th>
                        <th>Email</th>
                       @* <th>Website</th>*@
                        <th>Comment2</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr class="active">
                            <td>
                                <div class="btn btn-info btn-rounded mr-1 "><span class="glyphicon glyphicon-edit" style="font-size: 13px;" data-original-title="Edit">@Html.ActionLink("Edit", "Edit", "Enquiry", new { id = item.Id }, null) </span></div>
                            </td>
                            <td>@Convert.ToDateTime(item.Leaddate).ToString("dd/MM/yyyy")</td>
                            <td>@Html.DisplayFor(m => item.LeadName)@Html.HiddenFor(m => item.Id)</td>
                            <td>@Html.DisplayFor(m => item.Mobile)</td>
                            <td>@Html.DisplayFor(m => item.selectedinterestedcourse)</td>
                            <td>@Html.DisplayFor(m => item.selectedrefby)</td>
                            <td>@Html.DisplayFor(m => item.Comment1)</td>
                            <td>@Html.DisplayFor(m => item.selectedstatus)</td>
                            <td>@Html.DisplayFor(m => item.LeadOwner)</td>
                            <td>@Convert.ToDateTime(item.Followupdate).ToString("dd/MM/yyyy")</td>
                            <td>@Html.DisplayFor(m => item.LeadAssignedby)</td>
                            <td>@Html.DisplayFor(m => item.Email)</td>
                            @*<td>@Html.DisplayFor(m => item.Website)</td>*@
                            <td>@Html.DisplayFor(m => item.Comment2)</td>

                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>




